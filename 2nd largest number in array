import java.util.Arrays;

public class numbers{
public static void main(String[] args) {
int l = 0;
int s = 0;
int[] arr = {9,2,1,60,47};
for (int i =0;i<arr.length;i++) {
if (arr[i] > l) {
s = l;
l = arr[i];
} else if (arr[i] > s) {
s= arr[i];
}
}
System.out.println(Arrays.toString(arr));
System.out.println("The 2nd Largest number is :"+s");
}
}
